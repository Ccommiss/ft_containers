	"editor.semanticTokenColorCustomizations": {
		"rules": 
			{
				"storage.type" : {
					"foreground": "#FF0000",
					"fontStyle": "bold"
				}
			}
		
	},
	"editor.tokenColorCustomizations": {
		"textMateRules": [
			{
				"scope": [
					"comment.block",
					"punctuation.definition.comment.begin",
					"punctuation.definition.comment.end"
				],
				"settings": {
					"fontStyle": "bold",
					"foreground": "#7279b8"
				}
			},
			{
				"scope": [
					"storage.type.template.cpp",
				],
				"settings": {
					"fontStyle": "bold",
					"foreground": "#7abfce"
				}
			},
			{
				"scope": [
					"comment.line",
					"punctuation.definition.comment"
				],
				"settings": {
					"foreground": "#afafaf",
					"fontStyle": "bold"
				}
			},
			{
				"scope": [
					"meta.function-call.c",
					"meta.body.function.definition.cpp",

				],
				"settings": {
					"foreground": "#aaa",
					"fontStyle": "bold"
				}
			},
			{
				"scope": [
					"meta.function.definition.parameters.c",
					"entity.name.function.definition.cpp",
					"entity.name.function"
				],
				"settings": {
					"fontStyle": "bold",
					"foreground": "#dfaeae"
				}
			},
			{
				"scope": [
					"meta.function.definition.parameters"
				],
				"settings": {
					"fontStyle": "bold"
				}
			},
			{
				"scope": [
					"comment.block.c"
				],
				"settings": {
					"foreground": "#7279b8",
					"fontStyle": "bold"
				}
			},
			{
				"scope": [
					"variable.other.local"
				],
				"settings": {
					"foreground": "#FFF4AE"
				}
			},
			{
				"scope": [
					"variable.other.enummember"
				],
				"settings": {
					"foreground": "#C3DAC3"
				}
			},
			{
				"scope": [
					"variable.parameter"
				],
				"settings": {
					"foreground": "#BDD2A6",
					"fontStyle": "bold"
				}
			},
			{
				"scope": [
					"keyword.control.c",
					"keyword.control.cpp",
					"keyword.operator.comparison.cpp",
					"keyword.operator.logical.c",
					"keyword.operator.comparison.c"
				],
				"settings": {
					"fontStyle": "bold",
					"foreground": "#b1d8e6"
				}
			},
			{
				"scope": [
					"storage.type.built-in.primitive.cpp",
					"entity.name.type.cpp",
					"entity.name.scope-resolution.template.call.cpp",
					"entity.name.scope-resolution.cpp"
				],
				"settings": {
					"foreground": "#b8e9cb",
					"fontStyle": ""
				}
			}
	
		],

